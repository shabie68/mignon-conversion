{
    "name": "mignon/mignon-conversion",
    "description": "Y",
    "keywords": [
        "Mignon",
        "mignon-conversion"
    ],
    "homepage": "https://github.com/mignon/mignon-conversion",
    "license": "MIT",
    "authors": [
        {
            "name": "Shabie",
            "email": "shabeeulhassan40@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "pestphp/pest": "^1.20",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Mignon\\MignonConversion\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mignon\\MignonConversion\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
